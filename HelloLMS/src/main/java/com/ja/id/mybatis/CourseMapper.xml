<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ja.id.mybatis.CourseMapper">


	<select id="getAllCourse" resultType="java.util.HashMap"
		parameterType="java.util.HashMap">

		select coxseq, coxcate, coxname, coxstart,
		coxend,
		coxreqstart,
		coxreqend, tuxseq, coxintro, regdate
		from course 
		



	</select>
	<select id="getAllapply" resultType="java.util.HashMap"
		parameterType="java.util.HashMap">

		select axseq , coxseq , mxseq,
		axresult
		from apply

	</select>


</mapper>